def add():
    a = int(input("Enter the first number : "))
    b = int(input("Enter the second number : "))
    c = a+b
    print("Sum of the numbers is : ",c)

def subt():
    a = int(input("Enter the first number : "))
    b = int(input("Enter the second number :"))
    c = a-b
    print("Subtraction of the numbers is : ",c)
